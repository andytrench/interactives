{
"version": 2,
"builds": [
{
"src": "server.js",
"use": "@vercel/node"
},
{
"src": "public/**/*",
"use": "@vercel/static"
}
],
"routes": [
{
"src": "/settings/(.*)",
"dest": "/public/settings/$1"
},
{
"src": "/js/(.*)",
"dest": "/public/js/$1"
},
{
"src": "/(.*)",
"dest": "/public/$1"
}
],
"env": {
"NODE_ENV": "production"
}
}